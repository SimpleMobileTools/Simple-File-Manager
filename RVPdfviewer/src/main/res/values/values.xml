<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="pdfViewPager_ab_color">#990000</color>
    <dimen name="view_pager_side_margin">16dp</dimen>
    <string name="app_name">pdfviewer</string>
    <string name="pdfvp_ready_to_download">Ready to download</string>
    <declare-styleable name="PDFViewPager">
        <attr format="string" name="assetFileName"/> 
        <attr format="string" name="pdfUrl"/> 
        <attr format="float" name="scale"/> 
    </declare-styleable>
    <declare-styleable name="SubsamplingScaleImageView">
        <attr format="reference" name="src"/>
        <attr format="string" name="assetName"/>
        <attr format="boolean" name="panEnabled"/>
        <attr format="boolean" name="zoomEnabled"/>
        <attr format="boolean" name="quickScaleEnabled"/>
        <attr format="color" name="tileBackgroundColor"/>
    </declare-styleable>
</resources>
